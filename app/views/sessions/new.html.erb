<h1>Sign in</h1>
<div class="row">
	<div class="span4">
	<%= form_for(:session, :url => sessions_path, :html => { :class => 'well' }) do |f| %> 
		<div class="field">
			<%= f.label :email %>
			<%= f.text_field :email %> 
		</div>
		<div class="field"> 
			<%= f.label :password %>
			<%= f.password_field :password %>
		</div> 
		<div class="actions">
			<%= f.submit "Sign in", :class => 'btn-primary' %> 
		</div>
	<% end %> 
	</div>
	<div class="span8">
	</div>
</div>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>